      body {
        font-family:"Lucida Console", monospace;
        font-size: 12px;
        color:#000000;
        background:#ebebeb;
      }
      span#list-container {
        width: 60%;
        min-width: 435px;
        margin: 2.5% 0% 2.5% 0%;
        vertical-align: top;
        display: inline-block;
        border-radius: 25px;
        background-color: #E0E0E0;
      }
      span#search-container {
        text-align: center;
        width: 30%;
        min-width: 220px;
        max-width: 300px;
        margin: 5% 10% 0px 5%;
        vertical-align: top;
        display: inline-block;
        position: fixed;
      }
      div#main-container {
        max-width: 1000px;
        margin: auto;
      }
      ul#plugins, ul#modlist, ul#skyrim-ini, ul#skyrimprefs-ini {
        list-style-type: none;
        padding: 40px;
      }
      li {
        padding: 3px 0px 3px 3px;
        border-left: 1px solid #B0B0B0;
        border-right: 1px solid #B0B0B0;
      }
      li.grayed {
        background-color: #E0E0E0;
      }
      li.whited {
        background-color: #FFFFFF;
      }
      span.row-header {
        display: inline-block;
        text-align: right;
        vertical-align: top;
        width: 50px;
        padding: 0px 10px;
      }
      #plugins span.row-body {
        max-width: 75%;
        display: inline-block;
        vertical-align: top;
        overflow: hidden;
        white-space: nowrap;
      }
      span.row-footer {
        border-radius: 10px;
        display: inline-block;
        vertical-align: top;
        text-align: center;
        width: 40px;
        margin-right: 10px;
        float: right;
      }
      span.esp {
        background-color: rgba(0,140,0,0.25);
      }
      span.esm {
        background-color: rgba(0,0,140,0.25);
      }
      div#nav-wrapper {
        text-align: center;
        margin-top: 20px;
      }
      div#nav-wrapper button {
        background-color: #C0C0C0;
      }
    </style>

  </head>

