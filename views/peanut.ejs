              } else {
                $(this).slideUp();
              }
            }
          });
        });
      });

      function NavigateLists(_index) {
        if(_index != listoflistsindex) {
          $('#'+listoflists[listoflistsindex]).toggle('display', function() {
            $('#'+listoflists[listoflistsindex]+' > li').each(function(){ if(!$(this).is(":visible")) {$(this).show();}});
