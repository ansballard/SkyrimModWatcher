    <div id="main-container">
      <span id="list-container">
        <div id="nav-wrapper">
          <span><button class="btn" onclick="NavigateLists(0)">Plugins</button></span>
          <span><button class="btn" onclick="NavigateLists(1)">Modlist</button></span>
          <span><button class="btn" onclick="NavigateLists(2)">Skyrim</button></span>
          <span><button class="btn" onclick="NavigateLists(3)">SkyrimPrefs</button></span>
        </div>
        <ul class="" id="plugins"></ul>
        <ul class="" id="modlist"></ul>
        <ul class="" id="skyrim-ini"></ul>
        <ul class="" id="skyrimprefs-ini"></ul>
      </span>
      <span id="search-container">
        <h3 id="username"></h3>
        <input type="text" class="form-control" id="filter-mods" placeholder="Filter by..."/>
        <h3 id="num-mods">Number of Files</h3>
        <button class="btn" id="toggle">Next List</button>
      </span>
      
    </div>

